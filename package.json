{
  "name": "cursor-claude-connector",
  "version": "1.0.0",
  "description": "Connector proxy to use Cursor code editor with Claude (Pro/Max) subscription as API key for tokens",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/server.ts",
    "start": "node dist/server.js",
    "start:dev": "tsx watch src/server.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cursor",
    "claude",
    "anthropic",
    "openai",
    "proxy",
    "api",
    "connector",
    "code-editor",
    "claude-pro",
    "claude-max",
    "oauth"
  ],
  "author": "Maol-1997",
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.14.1",
    "hono": "^4.8.3"
  },
  "devDependencies": {
    "@types/bun": "^1.2.18",
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
